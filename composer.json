{
  "name": "boergenerwebdesign/bw-gallery",
  "type": "typo3-cms-extension",
  "description": "Erweitert Fluid um eine Galerie-Ansicht.",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": "^7.4 || ^8",
    "typo3/cms-core": "^11.5 || ^12.4",
    "wapplersystems/ws-scss": "*"
  },
  "replace": {
    "boergenerwebdesign/bw_gallery": "self.version",
    "typo3-ter/bw_gallery": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "bw_gallery"
    }
  },
  "autoload": {
    "psr-4": {
      "BoergenerWebdesign\\BwGallery\\": "Classes/"
    }
  }
}